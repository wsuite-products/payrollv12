<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_model_form_extended_inherit" model="ir.ui.view">
        <field name="model">ir.model</field>
        <field name="inherit_id" ref="base.view_model_form"/>
        <field name="arch" type="xml">
            <field name="modules" position="after">
                <field name="wdev_name"/>
            </field>
        </field>
    </record>

    <record id="view_model_tree_extended_inherit" model="ir.ui.view">
        <field name="model">ir.model</field>
        <field name="inherit_id" ref="base.view_model_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="wdev_name"/>
            </field>
        </field>
    </record>
</odoo>